{
  "name": "firestore-todo",
  "version": "1.0.0",
  "description": "firestore-todo",
  "main": "index.js",
  "scripts": {
    "dev": "next \"src/app\"",
    "firebase": "firebase",
    "deploy": "yarn firebase deploy --only hosting,functions:app",
    "build-public": "cpx \"src/public/**/*.*\" \"dist/public\" -C",
    "build-funcs": "babel \"src/functions\" --out-dir \"dist/functions\"",
    "build-app": "NODE_ENV=production next build \"src/app\"",
    "copy-deps": "cpx \"*{package.json,yarn.lock}\" \"dist/functions\"",
    "install-deps": "cd \"dist/functions\" && yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nabeliwo/firestore-todo.git"
  },
  "author": "nabeliwo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nabeliwo/firestore-todo/issues"
  },
  "homepage": "https://github.com/nabeliwo/firestore-todo#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "cpx": "^1.5.0",
    "firebase-tools": "^3.17.6"
  },
  "dependencies": {
    "firebase-admin": "^5.10.0",
    "firebase-functions": "^0.8.1",
    "mime": "^2.2.0",
    "next": "^5.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
